<?xml version="1.0" encoding="utf-8"?>
<Mapping Space="C-S" xmlns="http://schemas.microsoft.com/ado/2009/11/mapping/cs">
  <EntityContainerMapping StorageEntityContainer="sullyModelStoreContainer" CdmEntityContainer="sullyEntities">
    <EntitySetMapping Name="contactos">
      <EntityTypeMapping TypeName="sullyModel.contactos">
        <MappingFragment StoreEntitySet="contactos">
          <ScalarProperty Name="nombre" ColumnName="nombre" />
          <ScalarProperty Name="numero" ColumnName="numero" />
          <ScalarProperty Name="correo" ColumnName="correo" />
          <ScalarProperty Name="cedula" ColumnName="cedula" />
          <ScalarProperty Name="proveedorAsociada" ColumnName="proveedorAsociada" />
        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="estante">
      <EntityTypeMapping TypeName="sullyModel.estante">
        <MappingFragment StoreEntitySet="estante">
          <ScalarProperty Name="codigoBodega" ColumnName="codigoBodega" />
          <ScalarProperty Name="idTienda" ColumnName="idTienda" />
          <ScalarProperty Name="idProveedor" ColumnName="idProveedor" />
        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="factura">
