          <ScalarProperty Name="categoria" ColumnName="categoria" />
        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
    <AssociationSetMapping Name="productoProveedor" TypeName="sullyModel.productoProveedor" StoreEntitySet="productoProveedor">
      <EndProperty Name="producto">
        <ScalarProperty Name="idProducto" ColumnName="idProducto" />
      </EndProperty>
      <EndProperty Name="proveedor">
        <ScalarProperty Name="codigoProveedor" ColumnName="codigoProveedor" />
      </EndProperty>
    </AssociationSetMapping>
  </EntityContainerMapping>
</Mapping>