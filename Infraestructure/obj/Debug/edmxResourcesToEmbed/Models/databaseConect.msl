          <ScalarProperty Name="esAdmin" ColumnName="esAdmin" />
          <ScalarProperty Name="esActivo" ColumnName="esActivo" />
          <ScalarProperty Name="esEmpleado" ColumnName="esEmpleado" />
        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="productoFacturados">
      <EntityTypeMapping TypeName="sullyModel.productoFacturados">
        <MappingFragment StoreEntitySet="productoFacturados">
          <ScalarProperty Name="idProducto" ColumnName="idProducto" />
